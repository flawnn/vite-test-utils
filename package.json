{
  "name": "vite-test-utils",
  "version": "0.0.0",
  "description": "Test utils for Vite application",
  "keywords": [
    "vite",
    "vite.js",
    "test",
    "vitest",
    "e2e",
    "playwright"
  ],
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazupon/vite-test-utils.git"
  },
  "bugs": {
    "url": "https://github.com/kazupon/vite-test-utils/issues"
  },
  "engines": {
    "node": ">= 14.16"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "echo 'test!'",
    "build": "unbuild",
    "fix": "run-p lint:fix format:fix",
    "format": "prettier --config .prettierrc --ignore-path .prettierignore '**/*.{js,json,html}'",
    "format:fix": "pnpm format --write",
    "lint": "eslint --config .eslintrc.cjs --ext .js,.ts,.vue,.jsx,.tsx,.svelte,.json .",
    "lint:fix": "pnpm lint --fix"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@nexhome/yorkie": "^2.0.8",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jiti": "^1.14.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.6"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{json,md,yml}": [
      "prettier --write"
    ],
    "*.{js,vue}": [
      "prettier --write",
      "eslint --fix --ext .js,.vue"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix --ext .ts"
    ]
  }
}
